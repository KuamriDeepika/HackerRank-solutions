    // Complete the rotLeft function below.
    static int[] rotLeft(int[] array, int d) {
        int n=array.length;
        int[] arr = new int[array.length];
       for(int i=0; i<n;i++) {
            arr[i]=array[(i+d)%n]; // right rotation --->array[(i+n-d)%n]
        }
        return arr;
    }